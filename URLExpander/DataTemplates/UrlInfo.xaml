<UserControl x:Class="URLExpander.DataTemplates.UrlInfo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
    d:DesignHeight="55" d:DesignWidth="149" Loaded="UserControl_Loaded">
    <UserControl.Resources>
    </UserControl.Resources>
    <StackPanel>
        <HyperlinkButton NavigateUri="{Binding Path=Model.Url}">
            <TextBlock Foreground="#FF73A9D8" TextWrapping="Wrap" Text="{Binding Path=Model.Url}">
            </TextBlock>
        </HyperlinkButton>
        <TextBlock Text="{Binding Path=Model.NoC}" ></TextBlock>
    </StackPanel>

</UserControl>
